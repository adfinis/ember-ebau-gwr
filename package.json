    "ember-engines-router-service": "0.3.0",
    "ember-flatpickr": "^4.0.0",
    "ember-intl": "^5.7.2",
    "ember-power-select": "^7.0.0",
    "ember-resources": "^5.6.4",
    "ember-truth-helpers": "^3.1.1",
    "ember-uikit": "^8.0.0",
